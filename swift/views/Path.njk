import Foundation
import ObjectMapper

open class {{ className }}: Model {
    public override var pathSegment: String {
        get{
            return "{{ segment }}"
        }
    }

    {% for child in myChildren %}
      {% include "Child.njk" %}
    {% endfor %}

    {% for method in methods %}
        {% include "Method.njk" %}

        {% for definition in method.definitions %}
            {% include "Definition.njk" %}
        {% endfor %}
    {% endfor %}
}
